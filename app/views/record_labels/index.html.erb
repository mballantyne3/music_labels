<h1>Record Labels</h1>
<p><a href="/artists">Artists</a></p>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Created</th>
    </tr>
  </thead>
  <tbody>
    <% @record_labels.each do |label| %>
    <tr>
      <td><%= label.name %></td>
      <td><%= label.created_at %></td>
    </tr>
    <% end %>
  </tbody>
</table>
